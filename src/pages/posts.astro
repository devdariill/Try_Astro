---
const response = await fetch('https://jsonplaceholder.typicode.com/posts')
const users = await response.json()
export interface User {
  userId: number
  id: number
  title: string
  body: string
}
---

<body style="background-color: black; color:white;">
  <h1>Publicaciones</h1>
  {
    users.map((user: User) => {
      return (
        <div>
          <h5>{user.id}</h5>
          <h3>{user.title}</h3>
          <p>{user.body}</p>
        </div>
      )
    })
  }
</body>
